package dSuper_Class;

public class dAdmin extends cDeveloper{
	
	
	
	//Adding or Deleting the Product/Application
	public void manage()
	{
		read();
		write();
		System.out.println("Manage Code");
	}
	
	public void read()
	{
		System.out.println("Admin Read Code");
	}
	
